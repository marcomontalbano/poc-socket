{
  "name": "poc-socket",
  "version": "1.0.0",
  "description": "",
  "main": "public/index.html",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "lint": "tsc --noEmit",
    "test": "jest",
    "dev:server": "CORS_ORIGIN=http://localhost:1234 nodemon --ignore './demo/**' server.ts",
    "dev:demo": "parcel ./demo/index.html -d ./dist-demo",
    "dev": "concurrently yarn:dev:*",
    "build:server": "tsc --build tsconfig.build.json",
    "build:demo": "parcel build ./demo/index.html -d ./dist-demo --public-url /poc-socket/",
    "postbuild:demo": "touch ./dist-demo/.nojekyll",
    "build": "concurrently yarn:build:*",
    "serve:server": "node ./dist-server/server.js",
    "serve:demo": "npx serve ./dist-demo -p 1234",
    "serve": "concurrently yarn:serve:*",
    "heroku-postbuild": "yarn build:server"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/uuid": "^8.3.1",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.2.1",
    "jest": "^27.2.1",
    "nodemon": "^2.0.13",
    "parcel-bundler": "^1.12.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "wait-for-expect": "^3.0.2"
  },
  "engines": {
    "node": ">= 12"
  }
}
